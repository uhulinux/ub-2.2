	ftp://gcc.gnu.org/pub/gcc/releases/gcc-${UB_VERSION}/
	ftp://ftp.gnu.org/gnu/gcc/gcc-${UB_VERSION}/
	ftp://sources.redhat.com/pub/gcc/releases/gcc-${UB_VERSION}/
