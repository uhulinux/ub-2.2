_builddev()
{
        local cur foo
        cur=${COMP_WORDS[COMP_CWORD]}
        foo="$(/bin/ls /usr/src/UHUBUILD/UB-dev)"
        COMPREPLY=($(compgen -W "$foo" -- "$cur"))
}
complete -F _builddev build-dev

_build22()
{
        local cur foo
        cur=${COMP_WORDS[COMP_CWORD]}
        foo="$(/bin/ls /usr/src/UHUBUILD/UB-2.2)"
        COMPREPLY=($(compgen -W "$foo" -- "$cur"))
}
complete -F _build22 build-2.2
